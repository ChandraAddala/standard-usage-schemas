<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/event/support/account/sla"
               serviceCode="Support"
               version="1" resourceTypes="ACCOUNT_SUPPORT" 
               type="CREATE UPDATE">
    <description>
        Events describing serviceLevel changes of a supported account
    </description>
    <attribute name="serviceLevel" type="Name" use="required"
               allowedValues="MANAGED INFRASTRUCTURE INTENSIVE"> 
        The serviceLevel for this account
    </attribute>
    <attribute name="serviceType" type="Name" use="optional"
                   allowedValues="SYSOPS DEVOPS">
        The serviceType for this account
    </attribute>
</productSchema>
