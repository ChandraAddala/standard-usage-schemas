<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/event/domain"
               serviceCode="DomainRegistration"
               version="1" types="USAGE_SNAPSHOT"
               resourceTypes="DOMAIN_SUBSCRIPTION">
    <description>
        Domain registration events.
    </description>
    <attribute name="purchaseTenure" type="string"
               use="required">
        The purchase tenure.
    </attribute>
    <attribute name="domainTenure" type="int"
               use="required" min="1" max="9">
        The domain tenure.
    </attribute>
    <attribute name="expirationDate" type="utcDateTime" use="required">
        The date the domain registration expires.
    </attribute>
    <attribute name="renew" type="boolean"
               use="required">
        Is the subscription a renewal.
    </attribute>
</productSchema>