<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/usage/cloudbackup/bandwidthOut"
               serviceCode="CloudBackup"
               version="1"
               resourceTypes="AGENT">
    <description>
        Cloud Backup bandwith out usage information.
    </description>
    <attribute name="bandwidthOut" type="long" use="required"
               unitOfMeasure="B" aggregateFunction="SUM">
        The amount of bandwidth out, in bytes.
    </attribute>
    <attribute name="serverID" type="UUID" use="required">
        Server ID, where the agent in installed.
    </attribute>
</productSchema>
