<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/usage/queues/requestCount"
               serviceCode="CloudQueues"
               version="1"
               groupByResource="false">
    <description>
        Usage message for queue request count.
    </description>
    <attribute name="requestCount" type="unsignedLong" use="required" aggregateFunction="SUM"
            min="0" max="300000" unitOfMeasure="COUNT">
        The number of requests.  Max is 300,000.
    </attribute>

</productSchema>