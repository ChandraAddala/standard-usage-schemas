<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/usage/queues"
               serviceCode="CloudQueues"
               version="1"
               groupByResource="false">
    <description>
        Specifies the usage message for a queue request count.
    </description>
    <attribute name="requestCount" type="unsignedLong" use="required" aggregateFunction="SUM"
            min="0" max="3000000" unitOfMeasure="COUNT">
        Specifies the number of requests.  The maximum value is 3,000,000.
    </attribute>

</productSchema>
