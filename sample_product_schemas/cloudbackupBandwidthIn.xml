<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/usage/cloudbackup/bandwidthIn"
               serviceCode="CloudBackup"
               version="1"
               resourceTypes="AGENT">
    <description>
        Specifies usage information for the inbound Cloud Backup bandwith.
    </description>
    <attribute name="bandwidthIn" type="unsignedLong" use="required"
               min="0" max="18446744073709551615"
               unitOfMeasure="B" aggregateFunction="SUM">
        Specifies the amount of inbound bandwidth, in bytes.
    </attribute>
    <attribute name="serverID" type="int UUID" use="required">
        Specifies the Id of the server where the agent is installed.
    </attribute>
</productSchema>
