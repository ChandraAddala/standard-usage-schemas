<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/usage/queues/bandwidth"
               serviceCode="CloudQueues"
               version="1" resourceTypes="QUEUE">
    <description>
        Usage message for queue bandwidth.
    </description>
    <attribute name="bandwidthInPublic" type="unsignedLong" use="required"
               unitOfMeasure="B" aggregateFunction="SUM" min="0"
               max="6597069766656">
        The amount of public bandwidth in, in bytes. Max is 6TB.
    </attribute>
    <attribute name="bandwidthInServiceNet" type="unsignedLong" use="required"
               unitOfMeasure="B" aggregateFunction="SUM" min="0"
               max="6597069766656">
        The amount of bandwidth in on Service Net, in bytes. Max is 6TB.
    </attribute>
    <attribute name="bandwidthOutPublic" type="unsignedLong" use="required"
               unitOfMeasure="B" aggregateFunction="SUM" min="0"
               max="6597069766656">
        The amount of public bandwidth out, in bytes. Max is 6TB.
    </attribute>
    <attribute name="bandwidthOutServiceNet" type="unsignedLong" use="required"
               unitOfMeasure="B" aggregateFunction="SUM" min="0"
               max="6597069766656">
        The amount of bandwidth out on Service Net, in bytes. Max is 6TB.
    </attribute>
</productSchema>
