            <resource id="nova_usagesummary_events"        path="nova/events"    type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="queues_usagesummary_events"      path="queues/events"  type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="servers_usagesummary_events"     path="servers/events" type="wadl/feed.wadl#TenantAtomFeed"/>
