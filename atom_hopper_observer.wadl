        <resource path="usagedeadletter/events" type="wadl/feed.wadl#TenantAtomFeed"/>
        <resource path="usagerecovery/events" type="wadl/feed.wadl#TenantAtomFeed"/>
        <!-- Test related feeds -->
        <resource id="functest1_events" path="functest1/events" type="wadl/feed.wadl#TenantAtomFeed"/>
        <resource id="functest2_events" path="functest2/events" type="wadl/feed.wadl#TenantAtomFeed"/>

        <!-- Summary Feeds -->
        <resource path="usagesummary">
            <resource id="backup_usagesummary_events"      path="backup/events"  type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="bigdata_usagesummary_events"     path="bigdata/events" type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="cbs_usagesummary_events"         path="cbs/events"     type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="dbaas_usagesummary_events"       path="dbaas/events"   type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="dedicatedvcloud_sum_events"      path="dedicatedvcloud/events" type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="files_usagesummary_events"       path="files/events"   type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="glance_usagesummary_events"      path="glance/events"  type="wadl/feed.wadl#TenantAtomFeed"/>
            <resource id="lbaas_usagesummary_events"       path="lbaas/events"   type="wadl/feed.wadl#TenantAtomFeed"/>
